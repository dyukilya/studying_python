#задача 1
n=int(input())
for i in range (0,n+1):
    if i % 3 == 0 and i % 6 != 0:
        print (i)


#задача 2
n=int(input())
for i in range (10,n+1):
    if i % 2 == 0:
        print (i)


#задача 3
n=int(input())
if n % 2 == 0:
    a=0
n=int(input())
if n % 3 == 0:
    m=int(input())
    s=0
    for x in range (1,n+1):
        if x % m == 0:
            s+=1
    print(s)
else: 
    for i in range (1,n+1):
        print (n**i,end=" ")
print (end="\n")
